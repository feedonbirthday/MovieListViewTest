<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.movieListView">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/movie"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/movie"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.movieListView.ReservationPage"></activity>
        <activity android:name="com.example.movieListView.InformationPage" />
        <activity android:name="com.example.movieListView.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>